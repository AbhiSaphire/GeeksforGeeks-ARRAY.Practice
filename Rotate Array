#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n, d;
	    cin>>n>>d;
	    int array[n];
	    for (int i=0; i<n; i++){
	        cin>>array[n];
	    }
	    int array1[d];
	    for(int i=0; i<d; i++){
	        array1[i] = array[i];
	    }
	    for (int i=0 ; i<n-d ; i++, d++){
	        array[0] = array[d];
	    }
	    for (int i=0, j=d ; i<d, j<n ; i++, j++){
	        array[j] = array1[i];
	    }
	    for (int i=0; i<n; i++){
	        cout<<array[i]<<" ";
	    }
	    cout<<endl;
	}
	return 0;
}
